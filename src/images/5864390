<script type="text/javascript">
    var headbar = {
        ini:function(){
            headbar.my5lux_toggle();
            headbar.mobile_toggle();
            headbar.cart_toggle();
        },
        // 我的5lux
        my5lux_toggle:function(){
            $('.v30h_my5lux').mouseenter(function(){
                $('.v30h_my5lux').addClass('v30h_show');
                $('.v30h_my5lux>dd').show();
            });
            $('.v30h_my5lux').mouseleave(function(){
                $('.v30h_my5lux').removeClass('v30h_show');
                $('.v30h_my5lux>dd').hide();
            });
        },
        // mobile
        mobile_toggle:function(){
            $('.v30h_mobile>dt').mouseenter(function(){
                $('.v30h_mobile').addClass('v30h_show');
                $('.v30hm_qr').show();
            });
            $('.v30h_mobile').mouseleave(function(){
                $('.v30h_mobile').removeClass('v30h_show');
                $('.v30h_mobile>dd').hide();
            });
        },
        // cart
        cart_toggle:function(){
            $('.v30hm_cartbox>dt').mouseenter(function(){
                global.getcartinfo();
                $('.v30hm_cartbox>dd').show();
                $('.v30hm_cartbox>b').show();
            });
            $('.v30hm_cart').mouseleave(function(){
                $('.v30hm_cartbox>dd').hide();
                $('.v30hm_cartbox>b').hide();
            });
        }
    };
    headbar.ini();

</script>
<script>
<!--侧边导航栏关闭-->
$(function(){
        global.getcartinfo();
        infobox_showbox();
        $(".close_btn").click(function(e) {
        $(".main_siade").hide(300);
         var exp = new Date();
        Cookie.SetCookie('infobox_showbox_open',exp.getDate(),{expires:1});
    });
     
})
function infobox_showbox(){
    var index_showbox_open = Cookie.GetCookie('infobox_showbox_open');
    var exp = new Date();
    if(index_showbox_open!=exp.getDate()){
        $(".main_siade").show();
    }
}
</script>
