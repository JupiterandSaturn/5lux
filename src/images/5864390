<script>
$(function(){
    $(".leftmenu ul li").hover(
        function(){
            $(this).find('.menu_show').show().end().addClass('menuhover').siblings(".menu_show").hide();
            
            if($(this).find('.lipin').size()>0){
                var h1 = '-'+($(this).find('.menu_show').height()-32*2)+'px';
                $(this).find('.menu_show').css('top',h1);
            }
            if($(this).find('.bangong').size()>0){
                var h1 = '-'+($(this).find('.menu_show').height()-32*3)+'px';
                $(this).find('.menu_show').css('top',h1);
            }
            if($(this).find('.dianzi').size()>0){
                var h1 = '-'+($(this).find('.menu_show').height()-32*4)+'px';
                $(this).find('.menu_show').css('top',h1);
            }
            if($(this).find('.jiaju').size()>0){
                var h1 = '-'+($(this).find('.menu_show').height()-32*5)+'px';
                $(this).find('.menu_show').css('top',h1);
            }
            if($(this).find('.meishi').size()>0){
                var h1 = '-'+($(this).find('.menu_show').height()-32*6)+'px';
                    $(this).find('.menu_show').css('top',h1);
            }
            if($(this).find('.meijiu').size()>0){
                var h1 = '-'+($(this).find('.menu_show').height()-32*7)+'px';
                $(this).find('.menu_show').css('top',h1);
            }
            if($(this).find('.muying').size()>0){
                
                $(this).find('.menu_show').css('top','-226px');
            }
            if($(this).find('.xiexue').size()>0){
                var h1 = '-'+($(this).find('.menu_show').height()-32*8)+'px';
                $(this).find('.menu_show').css('top',h1);
            }
            if($(this).find('.peishi').size()>0){
                $(this).find('.menu_show').css('top','-32px');
            }
            if($(this).find('.wanbiao').size()>0){
                $(this).find('.menu_show').css('top','-32px');
            }
            if($(this).find('.fuzhuang').size()>0){
                $(this).find('.menu_show').css('top','-96px');
            }
            if($(this).find('.meizhuang').size()>0){
                $(this).find('.menu_show').css('top','-64px');
            }
            if($(this).find('.xiangbao').size()>0){
                $(this).find('.menu_show').css('top','-32px');
            }
            if($(this).find('.haiwai').size()>0){
                $(this).find('.menu_show').css('top','-222px');
            }
        },function(){
            $(this).find('.menu_show').hide().end().removeClass('menuhover');
        }
    );
    $('.nv_findstore').mouseover(function(){
        $(this).find('i').css('background-position','-366px -252px');
    });
    $('.nv_findstore').mouseout(function(){
