<script type="text/javascript">
        function _initImg() {
                var i = $('.cloudzoom');
                i.load(function() {
                        var _this = $(this);
                        var w = _this.width();
                        var h = _this.height();
                        var falg = w <= h ? true : false;
                        if (falg) {
                                _this.css({
                                        'width': 'auto',
                                        'height': 100 + '%'
                                });
                        } else {
                                _this.css({
                                        'width': 100 + '%',
                                        'height': 'auto'
                                });
                        };
                });
        };
        _initImg();
</script>    <!--商品相册end-->
