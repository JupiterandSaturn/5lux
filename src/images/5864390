<script type="text/javascript">
        $(window).ready(function() {
                var inx = 0;
                for (var i = 0; i < $('.banner-list').size(); i++) {
                        $('.num').append('<li class="pargaiton"></li>')
                }
                $('.pargaiton').eq(inx).addClass('active');
                // 自动轮播
                var t = setInterval(function() {
                        inx ++;
                        changeImg();
                }, 3000)
                function changeImg() {
//                        console.log(inx);
                        if (inx === $('.banner-list').size()) {

                                $('.banner-list').eq(0).fadeIn().siblings('.banner-list').fadeOut(900);
                                $('.pargaiton').eq(0).addClass('active').siblings().removeClass('active');
                                inx = 0;
                        } else if(inx === -1) {
                                $('.banner-list').eq($('.banner-list').size() - 1).fadeIn().siblings('.banner-list').fadeOut(900);
                                $('.pargaiton').eq($('.banner-list').size() - 1).addClass('active').siblings().removeClass('active');
                                inx = $('.banner-list').size() - 1;
                        } else {
                                $('.banner-list').eq(inx).fadeIn().siblings('.banner-list').fadeOut(900);
                                $('.pargaiton').eq(inx).addClass('active').siblings().removeClass('active');
                        }

                }
                $('#five-banner').hover(function() {
                        clearInterval(t);
                        $('.btn_l').animate({
                                'left': 10 + 'px',
                                'filter':'alpha(opacity=' + 100 + ')',
                                'opacity': 1
                        }, 300);
                        $('.btn_r').animate({
                                'right':10 + 'px',
                                'filter':'alpha(opacity=' + 100 + ')',
                                'opacity': 1
                        }, 300);
                },function() {
                        t = setInterval(function() {
                                inx ++;
                                changeImg();
                        }, 3000)
                        $('.btn_l').animate({
                                'left': -38 + 'px',
                                'filter':'alpha(opacity=' + 0 + ')',
                                'opacity': 0
                        }, 300);
                        $('.btn_r').animate({
                                'right': -38 + 'px',
                                'filter':'alpha(opacity=' + 0 + ')',
                                'opacity': 0
                        }, 300);
                });
                $('.pargaiton').click(function() {
                        inx = $(this).index();
                        changeImg();
                });
                $('.btn_l').click(function() {
                                inx--;
                                changeImg();
                });
                $('.btn_r').click(function() {
                        inx++;
                        changeImg();
                });
                // 控制图片缩放
                function changeSize(nodes, scles) {
                        nodes.hover(function() {
                                var Width = $(this).width();
                                var Heig = $(this).height();
                                var scle = Heig / Width;
                                var goalScle = scles;
                                $(this).children($('img')).css({
                                        'height': Heig,
                                        'width': Width
                                });
                                $(this).children($('img')).animate({
                                        'width': Width * goalScle + 'px',
                                        'height': Width * goalScle * scle + 'px',
                                        'marginLeft': -(Width * goalScle - Width) / 2,
                                        'marginTop': -(Width * goalScle * scle - Heig) / 2
                                });
                        }, function() {
                                $(this).children($('img')).animate({
                                        'width': 100 + '%',
                                        'height': 100 + '%',
                                        'marginLeft': 0,
                                        'marginTop': 0
                                });
                        });
                };
                changeSize($('.img-warp'), 1.05);
                var showMsg = [{
                            imgUrl: '/vendor/images/good/detail_desc/store_authentic_guarantee.png',
                            title: '专柜自提 正品保证',
                            texts: ['线上付款','线下自提','实现无缝对接','专柜正品保障']
                    },{
                            imgUrl: '/vendor/images/good/detail_desc/overseas_baoybaos.png',
                            title: '海外直发 包邮包税',
                            texts: ['与欧洲、美国、澳洲等多家正规授权时尚电商平台及买手店独家合作']
                    },{
                            imgUrl: '/vendor/images/good/detail_desc/chspafu_shlchengd.png',
                            title: '超时赔付 实力承诺',
                            texts: ['实行缺必赔','慢必赔政策','直接余额赔付','方便快捷']
                    },{
                            imgUrl: '/vendor/images/good/detail_desc/tuihbj_anqwy.png',
                            title: '退还便捷 安全无忧',
                            texts: ['退换货流程便利','海外产品也可实现退货','保证服务品质']
                    }];
                    $('.mark_close').click(function() {
                            var p = $(this).siblings('.ml_text');
                            $('.new_goodsmark').fadeOut(200, function() {
                                    for(var i = 0; i < p.length; i++) {
                                            p.eq(i).remove();
                                    };
                            });
                            return false;
                    });
                    $('.new_goodsmark').click(function() {
                            var p = $('.ml_text');
                            $('.new_goodsmark').fadeOut(200, function() {
                                    p.remove();
                            });
                            return false;
                    });
                    $('.overseals-body-list').click(function() {
                            var inx = $(this).index();
                            var texts = showMsg[inx].texts;
                            $('.mark_content > img').attr('src', showMsg[inx].imgUrl);
                            $('.mark_content > .mk_title').text(showMsg[inx].title);
                            for (var i = 0; i < texts.length; i++) {
                                    var str = '<p class="ml_text">' + texts[i] + '</p>'
                                    $('.mark_content').append(str);
                            }
                            $('.new_goodsmark').fadeIn(200);
                    })

        });
        // 广告位banner 居中
        $(window).load(function(){
            var goodsContent = $('.goodsContent_right').width();
            var bannerWidth = $('.autoimg').width();
            if (bannerWidth > goodsContent) {
                var offsetX = (bannerWidth - goodsContent) / 2;
                $('.autoimg').css({marginLeft: - offsetX + 'px' });
            }
        });
</script>
