        $(function(){
            //$('.disabled a').attr('href','javascript:void(0)');
            $('a.disabled').attr('href','javascript:void(0)');
            $('.color_size_cur a').attr('href','javascript:void(0)');

            $("#members_price").hover(function(){
                $('#member_con').show();
            },function(){
                $('#member_con').hide();
            });
            $("#member_con").hover(function(){
                $('#member_con').show();
            },function(){
                $('#member_con').hide();
            });
        });
</script>
<script type="text/javascript">
        $(".red-card").hover(function(e) {
            $(".red-card .todolist").show(50);
        },function(){
            $(".red-card .todolist").hide(50);
        });

        $(".qr-code").hover(function(e) {
            $(".qr-code img").show(50);
            $(".qr-code b").css({"background":"url(http://img550.5lux.com.cn/2017/06/13/qr/149734463161_12x26.png) no-repeat"});
        },function(){
            $(".qr-code img").hide(50);
            $(".qr-code b").css({"background":"url(http://img550.5lux.com.cn/2017/06/16/wx/149761259453_12x26.png) no-repeat"});
        });
</script>
<script>
    $(function(){
//        comment.getlist();//获取评论列表
        //数量减少
        $('#reducenum').click(function(){
            //var num = $(this).next().val();
            var num = $("#number").val();
            if(num < 2){
//                gf.alert('必须大于1');
                return false;
            }
            num = Number(num)-Number(1);
            $("#number").val(num);
            src = $('#go-buy').attr('href');
            src = src.replace(/&num=\d+/,'&num='+num);
            $('#go-buy').attr('href',src);
        });

        //增加数量
        $('#addnum').click(function(){
            var num = $("#number").val();
            var product_number = $("#product_number").val();
            product_number = Number(product_number);
            if(num >= product_number){
                gf.alert('对不起该商品的库存已经紧张或已存在购物车');return false;
            }
            num = Number(num)+Number(1);
            $("#number").val(num);
            src = $('#go-buy').attr('href');
            src = src.replace(/&num=\d+/,'&num='+num);
            $('#go-buy').attr('href',src);
        });

        $('#number').blur(function(){
            num = $(this).val();
            src = $('#go-buy').attr('href');
            src = src.replace(/&num=\d+/,'&num='+num);
            //alert(src);
            $('#go-buy').attr('href',src);
        });

        //添加商品到购物车
        $('.goods_add_fav').click(function(){
            var num = $("#number").val();
            var store_off_id = $('#store_off_id').val();
            var source = $('#click_source').val();
            global.addtocart('5864390',num,store_off_id,source);
        });

        // 给喜欢添加事件
        $('.add_collect_bt').click(function(ev){
            good.collect($(this).attr('title'),ev.pageX,ev.pageY);
        });
        //生成二维码
